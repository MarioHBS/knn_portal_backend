  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#ff8c4280",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#ffb366"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#ff6b1a",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#ff8c42"
      }
    },
    {
      "scope": "function",
      "settings": {
        "foreground": "#ffcc99"
      }
    },
    {
      "scope": "number",
      "settings": {
        "foreground": "#ff9933"
      }
    },
    {
      "scope": "type",
      "settings": {
        "foreground": "#ff6b1a"
      }
    }
